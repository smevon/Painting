public class Imagine {
	private String[][] Matrice;
	private int rows;
	private int cols;
	public int getRows() {
		return rows;
	}
	public void setRows(int rows) {
		this.rows = rows;
	}
	public int getCols() {
		return cols;
	}
	public void setCols(int cols) {
		this.cols = cols;
	}
	
	public void afisare() {
		for (int i = 0; i < this.getRows; i++) {
			for (int j = 0; j < this.getCols; j++)
				System.out.print(matrice[i][j]);
			System.out.println();
	}
}
